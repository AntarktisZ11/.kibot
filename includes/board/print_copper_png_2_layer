# Print the top and bottom copper layers in PNG format
kibot:
  version: 1

outputs:
  - name: 'print_copper_2_layer'
    comment: "Print all copper layers (2)"
    type: pcb_print
    dir: print_zones/png
    options:
      plot_sheet_reference: false
      format: 'PNG'
      keep_temporal_files: true
      pages:
        - monochrome: true
          layers: F.Cu
        - monochrome: true
          layers: B.Cu
